# Inherit Delphi's init.rc
import init.ihu_abl_car.rc

#setup network namespaces
on post-fs
    mkdir /dev/vendor/
    mkdir /dev/vendor/netns
    exec -- /vendor/bin/ip netns add vcc
    exec -- /vendor/bin/ip netns exec vcc /system/bin/sysctl -p /vendor/etc/sysctl.conf
    class_start vcc_net_init
