<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- The corner radius of the shadow behind the notification. -->
    <dimen name="notification_shadow_radius">12dp</dimen>

    <!-- The height of the divider between the individual notifications. -->
    <dimen name="notification_divider_height">2dp</dimen>

    <!-- The font size for the clock in the status bar. -->
    <dimen name="status_bar_clock_size">28sp</dimen>

    <!-- The amount to scale each of the status bar icons by. A value of 1 means no scaling. -->
    <item name="status_bar_icon_scale_factor" format="float" type="dimen">1.15</item>


    <dimen name="signal_cluster_margin_start">0dp</dimen>
    <dimen name ="signal_cluster_battery_padding">0dp</dimen>

    <!-- SignalDrawable -->
    <dimen name="signal_icon_size">25dp</dimen>

</resources>
