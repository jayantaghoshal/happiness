<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          coreApp="true"
          package="com.volvocars.remotectrlservices.generalsettingsgateway"
          android:exported="true"
          android:sharedUserId="android.uid.system">
    <original-package android:name="package com.volvocars.remotectrlservices.generalsettingsgateway"/>
    <permission
        android:name="android.volvocars.permission.REMOTE_CTRL"
        android:protectionLevel="system|signature"/>
    <application
        android:directBootAware="true"
        android:label="RemoteCtrl.GeneralSettings">

        <service
            android:name=".RemoteGeneralSettingsGateway"
            android:enabled="true"
            android:exported="true"
            android:persistent="true"
            android:singleUser="true">
            <intent-filter>
                <action android:name=".RemoteGeneralSettingsGateway.BIND"/>
            </intent-filter>
        </service>
    </application>
</manifest>





