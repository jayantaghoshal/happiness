cc_test {

    // A unique name for this module
    name: "VtsEvsExampleTest",

    // Inherit default settings and flags. See vendor/volvocars/defaults/Android.bp
    defaults: [
        "volvocars_cc_defaults_hq_tests",
    ],

    srcs: [
        "test_example.cpp",
    ],

    cppflags: [
        "-Werror",
    ],

    shared_libs: [
        "liblog",
    ],

    static_libs: [
        "libgtest",
    ],

    // Add this module to the VTS test suite
    test_suites: [
        "vts",
    ],
}
