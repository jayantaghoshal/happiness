cc_test {
    name: "libdbgTest",

    defaults: ["volvocars_cc_defaults"],

    srcs: [
        "libdbg_test.cpp",
    ],

    cppflags: [
        "-Werror",
        "-DLOG_TAG=\"libdgb\"",
        "-DLIBDBG_ENABLE_DEBUG",
    ],

    shared_libs: [
        "liblog",
        "libcutils",
    ],

    whole_static_libs: [
        "libdbg_static",
        "libgtest",
    ],

    test_suites: [
        "vts",
    ],

}
