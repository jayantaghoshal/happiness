<?xml version="1.0" encoding="utf-8"?>
<!--
This tests verifies the functionality in eventloop.
-->
<configuration description="Sound notifications unit tests">
    <target_preparer class="com.android.compatibility.common.tradefed.targetprep.VtsFilePusher">
        <option name="cleanup" value="true" />
        <option name="push" value="VtsSoundNotificationTest->/data/local/tmp/VtsSoundNotificationTest" />
    </target_preparer>
    <test class="com.android.tradefed.testtype.GTest" >
        <option name="native-test-device-path" value="/data/local/tmp" />
        <option name="module-name" value="VtsSoundNotificationTest" />
        <option name="runtime-hint" value="1s" />
    </test>
</configuration>
