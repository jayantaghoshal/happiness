<?xml version="1.0" encoding="utf-8"?>
<!-- DataElements unit test -->
<configuration description="Config for flexray signaling component test case, full vertical send and receive">
    <target_preparer class="com.android.compatibility.common.tradefed.targetprep.VtsFilePusher">
        <option name="push-group" value="HostDrivenTest.push"/>
        <option name="abort-on-push-failure" value="true"/>
        <option name="cleanup" value="true" />
        <option name="push" value="flexray_test->/data/local/tmp/flexray_test" />
    </target_preparer>
    <target_preparer class="com.android.tradefed.targetprep.VtsPythonVirtualenvPreparer"/>
    <test class="com.android.tradefed.testtype.VtsMultiDeviceTest">
           <option name="test-module-name" value="VtsFlexraySignalingCT_OneSendOneReceive"/>
           <option name="test-case-path" value="volvocars/hardware/signals/dataelements/test/ct/sendAndReceiveOneSignal/VtsFlexraySignalingCT_OneSendOneReceive"/>
           <option name="test-timeout" value="3m"/>
       </test>
</configuration>
