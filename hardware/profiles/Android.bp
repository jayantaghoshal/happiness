cc_binary {
    name: "vendor.volvocars.hardware.profiles@1.0-impl",
    relative_install_path: "hw",
    proprietary: true,
    defaults: ["volvocars_cc_defaults_hq"],
    init_rc: ["profilemanager.rc"],
    srcs: [
        "src/*.cpp",
    ],
    shared_libs: [
        "libhidlbase",
        "libhidltransport",
        "libutils",
        "liblog",
        "vendor.volvocars.hardware.profiles@1.0",
        "libdataelements",
        "libtarmac",
    ],
    tidy_checks: [
        "cppcoreguidelines-*",
        "-cppcoreguidelines-pro-type-vararg", // ALOG-macros use this
    ],
}
