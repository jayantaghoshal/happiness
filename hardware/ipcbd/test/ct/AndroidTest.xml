<?xml version="1.0" encoding="utf-8"?>
<!--
This tests verifies the that ipcbd is started in multiple instances.
-->
<configuration description="Test that ipcbd is started properly in three different instances">
<!--
    <target_preparer class="com.android.tradefed.targetprep.RunCommandTargetPreparer">
    </target_preparer>
-->
    <target_preparer class="com.android.compatibility.common.tradefed.targetprep.FilePusher">
        <option name="cleanup" value="true" />
        <option name="push" value="VtsIpcbCTCases->/data/local/tmp/VtsIpcbCTCases" />
    </target_preparer>

    <!-- Run test -->
    <test class="com.android.tradefed.testtype.GTest" >
        <option name="native-test-device-path" value="/data/local/tmp" />
        <option name="module-name" value="VtsIpcbCTCases" />
        <option name="runtime-hint" value="10s" />
    </test>
</configuration>
