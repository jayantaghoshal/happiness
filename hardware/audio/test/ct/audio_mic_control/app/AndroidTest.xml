<?xml version="1.0" encoding="utf-8"?>

<configuration description="Test to see if microphone mute is working">
    <target_preparer class="com.android.tradefed.targetprep.InstallBuildEnvApkSetup">
        <option name="apk-name" value="MicrophoneTestApp.apk" />
    </target_preparer>
    <test class="com.android.tradefed.testtype.AndroidJUnitTest" >
        <option name="package" value="com.volvocars.microphonetestapp" />
        <option name="runtime-hint" value="10s" />
    </test>
    <result_reporter class="com.android.tradefed.result.ConsoleResultReporter" />
    <result_reporter class="com.android.tradefed.result.XmlResultReporter" />
</configuration>
