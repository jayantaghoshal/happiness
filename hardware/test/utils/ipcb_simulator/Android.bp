//
// Static library. Used in shared lib and for unit tests.
//
cc_library_static {
    name: "libipcb_simulator_static",

    defaults: [
        "volvocars_cc_defaults",
    ],

    srcs: ["src/*"],

    shared_libs: [
        "libipcommandbus",
        "liblog",
        "libutils",
        "libtarmac",
        "liblocalconfig",
    ],

    local_include_dirs: ["include"],

    export_include_dirs: ["include"],

    compile_multilib: "64",
}
