cc_test {

    name: "VtsGnssIntegrationTest",

    srcs: [
        "VtsGnssIntegrationTest.cpp",
    ],

    defaults: [
        "volvocars_cc_defaults",
    ],

    cppflags: [
        "-DLOG_NDEBUG=0",
    ],

    shared_libs: [
        "liblog",
        "liblocalconfig",
        "libtarmac",
        "libutils",
        "libhidlbase",
        "libhidltransport",
        "libipcommandbus",
        "libhwbinder",
        "android.hardware.gnss@1.0",
        "vendor.volvocars.hardware.vehiclecom@1.0",
    ],

    static_libs: [
        "libgmock",
        "libipcb_simulator_static",
        "libinfotainmentbusasn_static",
    ],

    // Tag this module as a vts test artifact
    test_suites: ["vts"],

    // We only build for 64 bit.
    compile_multilib: "64",

}
