cc_binary{
   name: "carconfig-oneshot",
   proprietary: true,
   vendor: true,
   tidy: true,
      
   srcs: [
      "src/carconfig_setup.cpp",
      "src/carconfig_oneshot.cpp",
   ],
   
   cppflags: [
      "-std=c++14",
      "-Wno-non-virtual-dator",
      "-fexceptions",
      "-Wno-unused-parameters",
      "-Wno-macro-redefined",
   ],
   
   local_include_dirs: [
      "src",
   ],
   
   compile_multilib: "64",
}
