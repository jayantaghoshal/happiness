package vendor.volvocars.hardware.ipcb@1.0;

interface IResponseCallback
{
    /**
     * Handle response received over IPCB
     * @param resp Response received over IPCB.
     */
    oneway onResponseRcvd(Msg resp);

    /**
     * Handle error received over IPCB
     * @param error Error received over IPCB.
     */
    oneway onErrorRcvd(Error error);
};