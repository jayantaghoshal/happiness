     * @change_mode VehiclePropertyChangeMode:ON_CHANGE
     * @access VehiclePropertyAccess:READ
     * @data_enum PAStatus
     */
    EMERGENCY_LANE_KEEPING_AID_STATUS = (
      0x000D
      | VehiclePropertyGroup:VENDOR
      | VehiclePropertyType:INT32
      | VehicleArea:GLOBAL),

    EMERGENCY_LANE_KEEPING_AID_ON = (
      0x000E
      | VehiclePropertyGroup:VENDOR
      | VehiclePropertyType:BOOLEAN
      | VehicleArea:GLOBAL),

   /**
     * Toogles activation on ADAPTIVE CRUISE CONTROL functionality
     * @change_mode VehiclePropertyChangeMode:ON_CHANGE
     * @access VehiclePropertyAccess:READ_WRITE
     */
    ADAPTIVE_CRUISE_CONTROL_ON = (
      0x000F
      | VehiclePropertyGroup:VENDOR
      | VehiclePropertyType:BOOLEAN
      | VehicleArea:GLOBAL),

    /**
     * Current PAStatus for Adaptive Cruise Control
     * @change_mode VehiclePropertyChangeMode:ON_CHANGE
     * @access VehiclePropertyAccess:READ
     * @data_enum PAStatus
     */
    ADAPTIVE_CRUISE_CONTROL_STATUS = (
      0x0010
      | VehiclePropertyGroup:VENDOR
      | VehiclePropertyType:INT32
      | VehicleArea:GLOBAL),

    CRUISE_CONTROL_ON = (
      0x0011
      | VehiclePropertyGroup:VENDOR
      | VehiclePropertyType:BOOLEAN
      | VehicleArea:GLOBAL),

    /**
     * Current PAStatus for Cruise Control
     * @change_mode VehiclePropertyChangeMode:ON_CHANGE
     * @access VehiclePropertyAccess:READ
     * @data_enum PAStatus
     */
    CRUISE_CONTROL_STATUS = (
      0x0012
      | VehiclePropertyGroup:VENDOR
      | VehiclePropertyType:INT32
      | VehicleArea:GLOBAL),

    SPEED_LIMITER_ON = (
      0x0013
      | VehiclePropertyGroup:VENDOR
      | VehiclePropertyType:BOOLEAN
      | VehicleArea:GLOBAL),

    /**
     * Current PAStatus for Speed Limiter
     * @change_mode VehiclePropertyChangeMode:ON_CHANGE
     * @access VehiclePropertyAccess:READ
     * @data_enum PAStatus
     */
    SPEED_LIMITER_STATUS = (
      0x0014
      | VehiclePropertyGroup:VENDOR
      | VehiclePropertyType:INT32
      | VehicleArea:GLOBAL),

     /**
     * Toggles activation on Traffic sign information
     * @change_mode VehiclePropertyChangeMode:ON_CHANGE
     * @access VehiclePropertyAccess:READ_WRITE
     */
    TSI_RSI_ON = (
      0x0015
      | VehiclePropertyGroup:VENDOR
      | VehiclePropertyType:BOOLEAN
      | VehicleArea:GLOBAL),

    /**
     * Current PAStatus for Traffic sign information
     * @change_mode VehiclePropertyChangeMode:ON_CHANGE
     * @access VehiclePropertyAccess:READ
     * @data_enum PAStatus
     */
    TSI_RSI_STATUS = (
      0x0016
      | VehiclePropertyGroup:VENDOR
      | VehiclePropertyType:INT32
      | VehicleArea:GLOBAL),

    /**
     * Visual warning for TSI when the speed limit is exceeded
     * @change_mode VehiclePropertyChangeMode:ON_CHANGE
     * @access VehiclePropertyAccess:READ_WRITE
     */
    TSI_SPEED_VISUAL_WARN_ON = (
      0x0017
      | VehiclePropertyGroup:VENDOR
      | VehiclePropertyType:BOOLEAN
      | VehicleArea:GLOBAL),

    /**
