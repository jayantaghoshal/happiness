     * @change_mode VehiclePropertyChangeMode:ON_CHANGE
     * @access VehiclePropertyAccess:READ_WRITE
     */
    TSI_SPEED_AUDIO_WARN_ON = (
      0x0019
      | VehiclePropertyGroup:VENDOR
      | VehiclePropertyType:BOOLEAN
      | VehicleArea:GLOBAL),

